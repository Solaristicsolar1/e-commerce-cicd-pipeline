{
  "name": "ecommerce-build",
  "description": "Build project for e-commerce application",
  "source": {
    "type": "GITHUB",
    "location": "https://github.com/your-username/your-repo.git",
    "buildspec": "buildspec.yml"
  },
  "artifacts": {
    "type": "S3",
    "location": "codepipeline-us-east-1-artifacts"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/amazonlinux2-x86_64-standard:3.0",
    "computeType": "BUILD_GENERAL1_MEDIUM",
    "privilegedMode": true,
    "environmentVariables": [
      {
        "name": "AWS_DEFAULT_REGION",
        "value": "us-east-1"
      },
      {
        "name": "AWS_ACCOUNT_ID",
        "value": "192018386876"
      },
      {
        "name": "ECR_REPOSITORY_NAME",
        "value": "web-app"
      }
    ]
  },
  "serviceRole": "arn:aws:iam::192018386876:role/CodeBuildServiceRole"
}
